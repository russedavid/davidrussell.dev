{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.+\\.(png|jpg|jpeg|gif|ico|svg|webp))",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }